{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"outDir": "../../build/preload",
		"removeComments": true
	},
	"include": ["./**/*"],
	"exclude": ["node_modules/**/*", "./tsconfig.json", "./package.json"]
}
